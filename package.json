  "scripts": {
    "run": "node src/index.js",
    "build": "node build.js",
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "keywords": [
    "scribblehub",
    "books",
    "epub"
  ],
  "author": {
    "name": "Anna Damm",
    "email": "anna.damm87@googlemail.com",
    "url": "https://anna-damm.de/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnnaDamm/scribblehub-to-epub.git"
  },
  "license": "ISC",
  "dependencies": {
    "cli-progress": "^3.11.2",
    "commander": "^9.5.0",
    "epub-gen": "^0.1.0",
    "promise-throttle": "^1.1.2",
    "puppeteer": "^19.5.0",
    "yesno": "^0.4.0"
  },
  "homepage": "https://github.com/AnnaDamm/scribblehub-to-epub.git",
  "bugs": {
    "url": "https://github.com/AnnaDamm/scribblehub-to-epub.git/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
