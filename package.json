    "@babel/preset-env": "^7.20.2",
    "@webpack-cli/generators": "^3.0.1",
    "babel-loader": "^9.1.2",
    "compression-webpack-plugin": "^10.0.0",
    "standard": "^17.0.0",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0"
  },
  "babel": {
    "plugins": [
      "@babel/syntax-dynamic-import"
